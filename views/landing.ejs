
<%include partials/header.ejs %>
<h1> Curious people showcase </h1>


   <div class="container-fluid">
    <div class="row" style="text-align:center">
<% curiousList.forEach(function(curious){ %>
    <div class="col-xs-12 col-sm-6 col-lg-3 center-block">
    <h3> <%=curious.name %></h3>
    <img class=" img-responsive img-thumbnail" src="<%=curious.image%>">
    </div>
    
<%})%>
</div>
</div> 

